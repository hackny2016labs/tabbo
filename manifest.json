{
  "manifest_version": 2,

  "name": "Tabbo",
  "description": "Tab Management Hotkeys",
  "version": "1.0",
  "browser_action": {
    "default_popup": "tabbo.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["javascripts/vendor/jquery.min.js", "javascripts/vendor/mousetrap.min.js", "javascripts/content.js"]
    }
  ],
  "background": {
      "scripts": ["javascripts/background.js" ]
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/",
    "<all_urls>"
  ],
  "commands": {
      "test": {
          "suggested_key": {
              "default": "Ctrl+Shift+5"
          },
          "description": "stuff",
          "global": true
      }
  }
}
